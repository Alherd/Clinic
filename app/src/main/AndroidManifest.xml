<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.android.clinic">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/hospital1"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".ChooseActionActivity"/>
        <activity android:name=".MenuActivity"/>
        <activity android:name=".DatabaseDoctorsSignUpActivity"/>
        <activity android:name=".DatabaseServiceSignUpActivity"/>
        <activity android:name=".DatabaseNoteEntryActivity"/>
        <activity android:name=".AboutClinicActivity"/>
        <activity android:name=".ContactsClinicActivity"/>
        <activity android:name=".WebViewActivity"/>
        <activity android:name=".PasswordActivity"/>
        <activity android:name=".RegistrationActivity"/>
        <activity android:name=".DescriptionDoctorsActivity"/>
        <activity android:name=".DatabaseScheduleActivity"/>
        <activity android:name=".DatabaseCardPatientsActivity"/>
    </application>

</manifest>